@model IEnumerable<TurismoApp.Models.DestinoTuristico>

    @{
    ViewData["Title"] = "Destinos Turísticos";
    }

    <h1>Destinos Turísticos</h1>

    <table class="table">
        <thead>
            <tr>
                <th>Nome</th>
                <th>Descrição</th>
                <th>País</th>
            </tr>
        </thead>
        <tbody>
            @foreach (var item in Model)
        {
            <tr>
                <td>@item.Nome</td>
                <td>@item.Descricao</td>
                <td>@item.Pais</td>
            </tr>
        }
        </tbody>
    </table>
